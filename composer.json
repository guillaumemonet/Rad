{
    "name": "rad/rad-framework",
    "description": "Rapid Application Development Framework.",
    "authors": [
        {
            "name": "Guillaume Monet",
            "email": "guillaume.monet@free.fr"
        }
    ],

    "license": "MIT",

    "autoload": {
        "psr-4": {
            "Rad\\": "src/Rad/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rad/rad-framework",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/guillaumemonet/rad.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Rad\\": "src/Rad/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": " ^7.4",
        "psr/log": "^1",
        "psr/cache": "^1",
        "psr/container": "^1",
        "psr/http-message": "^1",
        "psr/link": "^1",
        "psr/simple-cache": "^1",
        "psr/http-server-middleware": "^1",
        "psr/http-server-handler": "^1",
        "psr/http-factory": "^1",
        "guzzlehttp/psr7": "2.4.3",
        "smarty/smarty": "v4.2.1"
    },
    "bin": ["bin/rad"]
}
