{
    "services": {
        "cache": {
            "default": "file",
            "handlers": {
                "file": {
                    "config": {
                        "path": "example/cache/"
                    }
                }
            }
        },
        "log": {
            "default": "file",
            "displayTime": true,
            "handlers": {
                "file": {
                    "config": {
                        "file": "example/log/api.log"
                    }
                }
            }
        }
    }
}